{
  "prelude": [
    "import * as Types from '../constants/types/crypto'",
    "import * as TeamBuildingTypes from '../constants/types/team-building'"
  ],
  "actions": {
    "clearRecipients": {
      "_description": "Remove all recipients from operation",
      "operation": "Types.Operations"
    },
    "clearInput": {},
    "setRecipients": {
      "_description": "Array recipients of operations, provided via TeamBuilding",
      "operation": "Types.Operations",
      "recipients": "Array<TeamBuildingTypes.User>"
    },
    "setInputType": {
      "operation": "Types.Operations",
      "type": "Types.InputTypes"
    },
    "setInputValue": {
      "operation": "Types.Operations",
      "value": "string"
    },
    "setOptions": {
      "operation": "Types.Operations",
      "options": "Types.OperationsOptions"
    }
  }
}
